{
	"_init_": { "exits": [ "waiting", "looking_right" ] },
	"waiting": {
		"signal" : "wait",
		"exits": [ "walking", "jumping" ]
	},
	"walking": {
		"signal" : "walk",
		"conditions" : [ "on_floor" ],
		"exits": [ "waiting" ]
	},
	"looking_right": {
		"signal" : "look_right",
		"conditions" : [ "on_floor" ],
		"exits": [ "looking_left" ]
	},
	"looking_left": {
		"signal" : "look_left",
		"conditions" : [ "on_floor" ],
		"exits": [ "looking_right" ]
	},
	"jumping": {
		"signal" : "jump_up",
		"conditions" : [ "on_floor" ],
		"exits": [ "falling" ]
	},
	"falling": {
		"signal" : "falling",
		"exits": [ "on_floor", "dead" ]
	},
	"on_floor": {
		"signal" : "on_floor",
		"exits": [ "jumping" ]
	},
	"reseting": {
		"signal" : "reset",
		"exits" : [ "_init_" ]
	},
	"paused": {
		"signal" : "pause",
		"exits" : [ "continue", "_finish_" ]
	},
	"_finish_": { "exits": [] }
}
